@* @model List<ObjetoEscuelaBase> *@

<div class="container">
    <h1>@ViewData["Title"]</h1>

    <table class="table table-dark table-hover">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in @Model)
            {
            <tr>
                <td>@obj.UniqueId</td>
                <td>@obj.Nombre</td>
            </tr>
            }
        </tbody>
    </table>
</div>